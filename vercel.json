{
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/",
      "dest": "/src/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/src/$1"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/src/404.html"
    }
  ]
}